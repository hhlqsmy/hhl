<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.linhs.springboot.mapper.UserMapper">

    <select id="listUser" resultType="com.linhs.springboot.pojo.User">
         select
              user_id as userId,
              user_name as userName
         from t_user
    </select>

</mapper>